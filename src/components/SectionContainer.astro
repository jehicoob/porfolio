---
interface Props {
    className?: string;
    id?: string;
}

const { className, id } = Astro.props;
---

<section
    id={id}
    data-section={id}
    class={`section ${className} scroll-m-20 w-full mx-auto my-10 px-6 md:px-0 container lg:max-w-4xl md:max-w-2xl flex flex-col gap-6`}
>
    <slot />
</section>
